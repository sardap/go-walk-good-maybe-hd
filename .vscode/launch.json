	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch game",
			"type": "go",
			"request": "launch",
			"preLaunchTask": "gennerate assets",
			"mode": "debug",
			"program": "${workspaceFolder}/cmd/wgmhd",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Launch gen Package",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/cmd/gen",
			"args": [
				"${workspaceFolder}/pkg",
				"dev"
			]
		},
		{
			"name": "Launch assetweb Package",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/assetweb",
			"args": [
				"${workspaceFolder}/out"
			]
		}
	]
}